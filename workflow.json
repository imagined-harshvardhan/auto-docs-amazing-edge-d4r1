    { "source": "doc_writer_agent", "target": "output_node" },
    { "source": "publisher_agent", "target": "output_node" },
    { "source": "code_reviewer_agent", "target": "output_node" },
    { "source": "onboarding_doc_writer", "target": "output_node" }
  ]
